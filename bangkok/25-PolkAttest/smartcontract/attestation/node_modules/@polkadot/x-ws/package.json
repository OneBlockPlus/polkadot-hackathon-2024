{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/common/issues",
  "description": "A cross-environment WebSocket replacement",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/polkadot-js/common/tree/master/packages/x-ws#readme",
  "license": "Apache-2.0",
  "name": "@polkadot/x-ws",
  "repository": {
    "directory": "packages/x-ws",
    "type": "git",
    "url": "https://github.com/polkadot-js/common.git"
  },
  "sideEffects": false,
  "type": "module",
  "version": "13.0.2",
  "main": "./cjs/node.js",
  "module": "./node.js",
  "browser": "./cjs/browser.js",
  "react-native": "./cjs/react-native.js",
  "types": "./node.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./node.d.ts",
      "react-native": {
        "module": "./react-native.js",
        "require": "./cjs/react-native.js",
        "default": "./react-native.js"
      },
      "browser": {
        "module": "./browser.js",
        "require": "./cjs/browser.js",
        "default": "./browser.js"
      },
      "node": {
        "module": "./node.js",
        "require": "./cjs/node.js",
        "default": "./node.js"
      }
    },
    "./browser": {
      "types": "./browser.d.ts",
      "module": "./browser.js",
      "require": "./cjs/browser.js",
      "default": "./browser.js"
    },
    "./node": {
      "types": "./node.d.ts",
      "module": "./node.js",
      "require": "./cjs/node.js",
      "default": "./node.js"
    },
    "./package.json": {
      "require": "./cjs/package.json",
      "default": "./package.json"
    },
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./react-native": {
      "types": "./react-native.d.ts",
      "module": "./react-native.js",
      "require": "./cjs/react-native.js",
      "default": "./react-native.js"
    },
    "./shim.js": {
      "types": "./shim.d.ts",
      "module": "./shim.js",
      "require": "./cjs/shim.js",
      "default": "./shim.js"
    },
    "./shim": {
      "types": "./shim.d.ts",
      "module": "./shim.js",
      "require": "./cjs/shim.js",
      "default": "./shim.js"
    }
  },
  "dependencies": {
    "@polkadot/x-global": "13.0.2",
    "tslib": "^2.6.2",
    "ws": "^8.16.0"
  }
}
