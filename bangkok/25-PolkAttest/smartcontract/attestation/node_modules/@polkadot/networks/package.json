{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/common/issues",
  "description": "A list of all available Substrate networks and their applicable prefixes",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/polkadot-js/common/tree/master/packages/networks#readme",
  "license": "Apache-2.0",
  "name": "@polkadot/networks",
  "repository": {
    "directory": "packages/networks",
    "type": "git",
    "url": "https://github.com/polkadot-js/common.git"
  },
  "sideEffects": false,
  "type": "module",
  "version": "13.0.2",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./defaults": {
      "types": "./defaults/index.d.ts",
      "module": "./defaults/index.js",
      "require": "./cjs/defaults/index.js",
      "default": "./defaults/index.js"
    },
    "./defaults/genesis": {
      "types": "./defaults/genesis.d.ts",
      "module": "./defaults/genesis.js",
      "require": "./cjs/defaults/genesis.js",
      "default": "./defaults/genesis.js"
    },
    "./defaults/icons": {
      "types": "./defaults/icons.d.ts",
      "module": "./defaults/icons.js",
      "require": "./cjs/defaults/icons.js",
      "default": "./defaults/icons.js"
    },
    "./defaults/ledger": {
      "types": "./defaults/ledger.d.ts",
      "module": "./defaults/ledger.js",
      "require": "./cjs/defaults/ledger.js",
      "default": "./defaults/ledger.js"
    },
    "./defaults/testnets": {
      "types": "./defaults/testnets.d.ts",
      "module": "./defaults/testnets.js",
      "require": "./cjs/defaults/testnets.js",
      "default": "./defaults/testnets.js"
    },
    "./interfaces": {
      "types": "./interfaces.d.ts",
      "module": "./interfaces.js",
      "require": "./cjs/interfaces.js",
      "default": "./interfaces.js"
    },
    "./package.json": {
      "require": "./cjs/package.json",
      "default": "./package.json"
    },
    "./packageDetect": {
      "types": "./packageDetect.d.ts",
      "module": "./packageDetect.js",
      "require": "./cjs/packageDetect.js",
      "default": "./packageDetect.js"
    },
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "module": "./types.js",
      "require": "./cjs/types.js",
      "default": "./types.js"
    }
  },
  "dependencies": {
    "@polkadot/util": "13.0.2",
    "@substrate/ss58-registry": "^1.46.0",
    "tslib": "^2.6.2"
  }
}
