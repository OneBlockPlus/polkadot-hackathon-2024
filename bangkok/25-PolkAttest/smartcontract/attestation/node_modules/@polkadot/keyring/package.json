{
  "author": "Jaco Greeff <jacogr@gmail.com>",
  "bugs": "https://github.com/polkadot-js/common/issues",
  "description": "Keyring management",
  "engines": {
    "node": ">=18"
  },
  "homepage": "https://github.com/polkadot-js/common/tree/master/packages/keyring#readme",
  "license": "Apache-2.0",
  "name": "@polkadot/keyring",
  "repository": {
    "directory": "packages/keyring",
    "type": "git",
    "url": "https://github.com/polkadot-js/common.git"
  },
  "sideEffects": [
    "./packageDetect.js",
    "./cjs/packageDetect.js"
  ],
  "type": "module",
  "version": "13.0.2",
  "main": "./cjs/index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    "./cjs/package.json": "./cjs/package.json",
    "./cjs/*": "./cjs/*.js",
    ".": {
      "types": "./index.d.ts",
      "module": "./index.js",
      "require": "./cjs/index.js",
      "default": "./index.js"
    },
    "./bundle": {
      "types": "./bundle.d.ts",
      "module": "./bundle.js",
      "require": "./cjs/bundle.js",
      "default": "./bundle.js"
    },
    "./defaults": {
      "types": "./defaults.d.ts",
      "module": "./defaults.js",
      "require": "./cjs/defaults.js",
      "default": "./defaults.js"
    },
    "./keyring": {
      "types": "./keyring.d.ts",
      "module": "./keyring.js",
      "require": "./cjs/keyring.js",
      "default": "./keyring.js"
    },
    "./package.json": {
      "require": "./cjs/package.json",
      "default": "./package.json"
    },
    "./packageDetect": {
      "types": "./packageDetect.d.ts",
      "module": "./packageDetect.js",
      "require": "./cjs/packageDetect.js",
      "default": "./packageDetect.js"
    },
    "./packageInfo.js": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./packageInfo": {
      "types": "./packageInfo.d.ts",
      "module": "./packageInfo.js",
      "require": "./cjs/packageInfo.js",
      "default": "./packageInfo.js"
    },
    "./pair": {
      "types": "./pair/index.d.ts",
      "module": "./pair/index.js",
      "require": "./cjs/pair/index.js",
      "default": "./pair/index.js"
    },
    "./pair/decode": {
      "types": "./pair/decode.d.ts",
      "module": "./pair/decode.js",
      "require": "./cjs/pair/decode.js",
      "default": "./pair/decode.js"
    },
    "./pair/defaults": {
      "types": "./pair/defaults.d.ts",
      "module": "./pair/defaults.js",
      "require": "./cjs/pair/defaults.js",
      "default": "./pair/defaults.js"
    },
    "./pair/encode": {
      "types": "./pair/encode.d.ts",
      "module": "./pair/encode.js",
      "require": "./cjs/pair/encode.js",
      "default": "./pair/encode.js"
    },
    "./pair/nobody": {
      "types": "./pair/nobody.d.ts",
      "module": "./pair/nobody.js",
      "require": "./cjs/pair/nobody.js",
      "default": "./pair/nobody.js"
    },
    "./pair/toJson": {
      "types": "./pair/toJson.d.ts",
      "module": "./pair/toJson.js",
      "require": "./cjs/pair/toJson.js",
      "default": "./pair/toJson.js"
    },
    "./pair/types": {
      "types": "./pair/types.d.ts",
      "module": "./pair/types.js",
      "require": "./cjs/pair/types.js",
      "default": "./pair/types.js"
    },
    "./pairs": {
      "types": "./pairs.d.ts",
      "module": "./pairs.js",
      "require": "./cjs/pairs.js",
      "default": "./pairs.js"
    },
    "./testing": {
      "types": "./testing.d.ts",
      "module": "./testing.js",
      "require": "./cjs/testing.js",
      "default": "./testing.js"
    },
    "./testingPairs": {
      "types": "./testingPairs.d.ts",
      "module": "./testingPairs.js",
      "require": "./cjs/testingPairs.js",
      "default": "./testingPairs.js"
    },
    "./types": {
      "types": "./types.d.ts",
      "module": "./types.js",
      "require": "./cjs/types.js",
      "default": "./types.js"
    }
  },
  "dependencies": {
    "@polkadot/util": "13.0.2",
    "@polkadot/util-crypto": "13.0.2",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "@polkadot/util": "13.0.2",
    "@polkadot/util-crypto": "13.0.2"
  }
}
