{
	"compilerOptions": {
		"module": "commonjs",
    	"moduleResolution": "node",
		"target": "ES2020",
		"lib": ["es6", "dom"],
    	"jsx": "react-jsx",
		"sourceMap": true,
		"rootDir": "src",
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noUnusedParameters": true,
    	"allowJs": true,
    	"esModuleInterop": true
	},
  "exclude": ["node_modules", ".vscode-test", "out", "scripts", "assets"]
}
