{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:prettier/recommended"
  ],
  "plugins": [
  ],
  "rules": {
    "@typescript-eslint/explicit-module-boundary-types": 0,
    "semi": [
      0,
      "always"
    ]
  }
}
